package com.droidbrew.javakoans.eagkoans;

import static org.junit.Assert.*;
import static com.droidbrew.javakoans.eagkoans.base.Koan.*;

import org.junit.Test;



public class A_AboutAnnotationsBasics {

	@interface RedMagnet{
	}
	
	@RedMagnet
	Object SomeData;
	
	@RedMagnet
	@Test
	public void a_____in_the_very_basics_annotation_is_just_a_metadata_marker() {
		@RedMagnet Object SomeData = new Object();
		//assertEquals(___, RedMagnet.class.getSimpleName());
		assertEquals(___, SomeData.getClass().getAnnotations());
	}

}
